{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "import/extensions": "off",
    }
}
